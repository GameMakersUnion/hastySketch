material StickmanMaterial
{
	technique
	{
		pass
		{
           scene_blend alpha_blend
           depth_write off

			texture_unit
			{
				texture stickman.png
                alpha_op_ex modulate src_texture src_current
                scroll_anim 0.5 0.0
                rotate 180
			}
            
           // scene_blend modulate

		}
	}
}